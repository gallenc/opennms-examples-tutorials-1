<?xml version="1.0"?>
<!-- Syslog message match definitions for OpenSSH -->
<syslogd-configuration-group>
   <ueiList>

      <!-- note that these matches depend on pri severities -->
      <ueiMatch>
         <facility>local7</facility>
         <process-match expression="^notfmgrd$" />
         <severity>Emergency</severity>
         <match type="regex" expression="(?s)\[(.*?)\]\s(.*?)Id:(.*?),\sSyslog-Severity:([0-9]+),\sPerceived-Severity:(.*?),\sName:(.*?),\sCategory:(.*?)\sCause:(.*?),\sDetails:(.*?),\sXpath:(.*?)\sAddress:(.*?),\s(.*)" />
         <parameter-assignment matching-group="1" parameter-name="LogFacility"/>
         <parameter-assignment matching-group="2" parameter-name="Event"/>
         <parameter-assignment matching-group="3" parameter-name="Id"/>
         <parameter-assignment matching-group="4" parameter-name="SyslogSeverity"/>
         <parameter-assignment matching-group="5" parameter-name="PerceivedSeverity"/>
         <parameter-assignment matching-group="6" parameter-name="LogName"/>
         <parameter-assignment matching-group="7" parameter-name="LogCategory"/>
         <parameter-assignment matching-group="8" parameter-name="LogCause"/>
         <parameter-assignment matching-group="9" parameter-name="Details"/>
         <parameter-assignment matching-group="10" parameter-name="Xpath"/>
         <parameter-assignment matching-group="11" parameter-name="Address"/>
         <parameter-assignment matching-group="12" parameter-name="AdditionalInfo"/>
         
         <uei>uei.opennms.org/Calex/Axos/local7/Emergency</uei>
      </ueiMatch>
