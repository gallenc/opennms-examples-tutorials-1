      <!-- Translations for Calex ONT Devices -->
      <!-- ******************************************* -->
      <!-- Allowed severities see mapping in https://www.rfc-editor.org/rfc/rfc5674.html Alarms in Syslog -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Emergency</uei> CRITICAL -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Alert</uei> CRITICAL -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Critical</uei> MAJOR -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Error</uei> MINOR -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Warning</uei> WARNING -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Notice</uei> INDETERNINATE -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Informational</uei> CLEARED -->
      <!-- <uei>uei.opennms.org/Calex/Axos/Syslog/Debug</uei> INDETERNINATE -->

      <!-- OpenNMS severities -->
      <!-- INDETERMINATE(1, "Indeterminate", "lightblue"), -->
      <!-- CLEARED(2, "Cleared", "white"), -->
      <!-- NORMAL(3, "Normal", "green"), -->
      <!-- WARNING(4, "Warning", "cyan"), -->
      <!-- MINOR(5, "Minor", "yellow"), -->
      <!-- MAJOR(6, "Major", "orange"), -->
      <!-- CRITICAL(7, "Critical", "red"); -->

      <event-translation-spec uei="uei.opennms.org/Calex/Axos/Syslog/Emergency">
         <mappings>
            <mapping>
               <assignment name="uei" type="field">
                  <value type="constant" result="uei.opennms.org/Translator/Calex/Axos/ONT/Alarm/Raise" />
               </assignment>
               <assignment name="severity" type="field">
                  <value type="constant" result="Critical" />
               </assignment>
                <assignment name="interface" type="field"> 
                   <value type="constant" result="254.0.0.1" />
                </assignment>
               <assignment name="nodeid" type="field">
                  <value type="sql" result="SELECT a.nodeid FROM assets a WHERE a.assetnumber =?">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="originalEventId" type="parameter">
                  <value type="field" name="dbid" matches=".*" result="${0}" />
               </assignment>
            </mapping>
         </mappings>
      </event-translation-spec>

