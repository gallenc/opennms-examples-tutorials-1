                </assignment>
               <assignment name="nodeid" type="field">
                  <value type="sql" result="SELECT a.nodeid FROM assets a WHERE a.assetnumber =?">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="originalEventId" type="parameter">
                  <value type="field" name="dbid" matches=".*" result="${0}" />
               </assignment>
            </mapping>
            
            <!-- *** SAME BLOCK IN ALL ALARM RAISE TRANSLATION SPECS *** -->
            <!-- mapping for SECONDARY_NODE (parent of ONT)-->
            <mapping>
               <assignment name="uei" type="field">
                  <value type="constant" result="uei.opennms.org/Translator/gponSplitter/Alarm/Raise" />
               </assignment>
               <assignment name="severity" type="field">
                     <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n where n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =? ) ), 
                     alarm_count_query AS (SELECT COUNT(*) AS alarmCount FROM alarms al WHERE al.severity>3 AND al.reductionkey LIKE CONCAT( '%:',?) AND al.nodeid IN (SELECT n.nodeid FROM node n where n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query))),
                     child_count_query AS (SELECT COUNT(*) AS childCount FROM node n WHERE n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query) )
                     SELECT (CASE WHEN alarmCount >= childCount THEN 'Major' ELSE 'Warning' END ) FROM  child_count_query,alarm_count_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                     <value type="parameter" name="LogName" matches=".*" result="${0}" />
                  </value>
               </assignment>
                <assignment name="interface" type="field"> 
                   <value type="constant" result="254.0.0.1" />
                </assignment>
               <assignment name="nodeid" type="field">
                  <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n where n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =? ) )
                     SELECT parentId FROM ont_parent_id_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="sameChildAlarms" type="parameter">
                     <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n where n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =? ) ), 
                     alarm_count_query AS (SELECT COUNT(*) AS alarmCount FROM alarms al WHERE al.severity>3 AND al.reductionkey LIKE CONCAT( '%:',?) AND al.nodeid IN (SELECT n.nodeid FROM node n where n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query)))
                     SELECT alarmCount FROM  alarm_count_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                     <value type="parameter" name="LogName" matches=".*" result="${0}" />
                  </value>
               </assignment>
               <assignment name="numberOfChildren" type="parameter">
                     <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n where n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =? ) ), 
                     child_count_query AS (SELECT COUNT(*) AS childCount FROM node n WHERE n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query) )
                     SELECT childCount  FROM  child_count_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="originalEventId" type="parameter">
                  <value type="field" name="dbid" matches=".*" result="${0}" />
               </assignment>
            </mapping>
            
            <!-- mapping for PRIMARY_NODE (parent of SECONDARY_NODE) -->
            <mapping>
               <assignment name="uei" type="field">
                  <value type="constant" result="uei.opennms.org/Translator/gponSplitter/Alarm/Raise" />
               </assignment>
               <assignment name="severity" type="field">
                     <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n WHERE n.nodeid IN ( SELECT n.nodeparentid FROM node n WHERE n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =?) ) ), 
                     alarm_count_query AS (SELECT COUNT(*) AS alarmCount FROM alarms al WHERE al.severity>5 AND al.reductionkey LIKE CONCAT( '%:',?) AND al.nodeid IN (SELECT n.nodeid FROM node n where n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query))),
                     child_count_query AS (SELECT COUNT(*) AS childCount FROM node n WHERE n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query) )
                     SELECT (CASE WHEN alarmCount >= childCount THEN 'Major' ELSE 'Warning' END ) FROM  child_count_query,alarm_count_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                     <value type="parameter" name="LogName" matches=".*" result="${0}" />
                  </value>
                </assignment>
                <assignment name="interface" type="field"> 
                   <value type="constant" result="254.0.0.1" />
                </assignment>
               <assignment name="nodeid" type="field">
                  <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n WHERE n.nodeid IN ( SELECT n.nodeparentid FROM node n WHERE n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =?) ) )
                  SELECT parentId FROM ont_parent_id_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="sameChildAlarms" type="parameter">
                   <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n WHERE n.nodeid IN ( SELECT n.nodeparentid FROM node n WHERE n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =?) ) ), 
                     alarm_count_query AS (SELECT COUNT(*) AS alarmCount FROM alarms al WHERE al.severity>5 AND al.reductionkey LIKE CONCAT( '%:',?) AND al.nodeid IN (SELECT n.nodeid FROM node n where n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query)))
                     SELECT alarmCount FROM  alarm_count_query ">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                     <value type="parameter" name="LogName" matches=".*" result="${0}" />
                  </value>
               </assignment>
               <assignment name="numberOfChildren" type="parameter">
                     <value type="sql" result="WITH ont_parent_id_query AS (SELECT n.nodeparentid AS parentId FROM node n WHERE n.nodeid IN ( SELECT n.nodeparentid FROM node n WHERE n.nodeid IN ( SELECT a.nodeid FROM assets a WHERE a.displaycategory='ONT' AND a.assetnumber =?) ) ), 
                     child_count_query AS (SELECT COUNT(*) AS childCount FROM node n WHERE n.nodeparentid IN (SELECT parentId FROM ont_parent_id_query) )
                     SELECT childCount  FROM  child_count_query">
                     <value type="parameter" name="Xpath" matches="(.*?)ont-id='(.*?)'(.*?)" result="${2}" />
                  </value>
               </assignment>
               <assignment name="originalEventId" type="parameter">
                  <value type="field" name="dbid" matches=".*" result="${0}" />
               </assignment>
            </mapping>
            <!-- *** END OF SAME BLOCK IN ALL ALARM RAISE TRANSLATION SPECS *** -->
                        
         </mappings>
      </event-translation-spec>

